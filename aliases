alias gaa='git add -A'
alias gcm='git commit -m'
alias gct='git checkout'
alias gits='git status'
alias gp='git push'
alias gpl='git pull -all'
alias grep='grep --color=auto'
alias h='history'
alias head='sed 11q'
alias ipy='ipython'
alias irc='irssi'
alias k='clear'
alias l1='ls -1'
alias l='ls -a'
alias la='ls -AF'
