alias vbrc='$EDITOR $HOME/.bashrc'
alias vi3c='$EDITOR $HOME/.config/i3/config'
alias vvrc='$EDITOR $HOME/.vimrc'
alias wgetall='wget --execute=\"robots = off\" --mirror --convert-links --no-parent --wait=5'
